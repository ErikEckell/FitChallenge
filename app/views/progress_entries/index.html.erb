<h1>Progress Entries</h1>

<table class="table table-striped table-hover align-middle">
  <thead>
    <tr>
      <th>Participation</th>
      <th>Exercise</th>
      <th>Date</th>
      <th>Metric Value</th>
      <th>Points</th>
      <th>Notes</th>
    </tr>
  </thead>
  <tbody>
    <% @progress_entries.each do |entry| %>
      <tr>
        <td><%= entry.participation.challenge.title %></td>
        <td><%= entry.exercise.name %></td>
        <td><%= entry.entry_date %></td>
        <td><%= entry.metric_value %></td>
        <td><%= entry.points %></td>
        <td><%= entry.notes %></td>
      </tr>
    <% end %>
  </tbody>
</table>
